<!-- SQL definition -->
<mapper namespace="UserMapper07">
	<select id="getUserList" parameterType="user" resultType="user">
		select 
		user_id as userId
		,user_name as userName
		,password as password 
		,age as age 
		,grade as grade
		,reg_date as regDate
		from users
		where user_id=#{userId}
	</select>
</mapper>